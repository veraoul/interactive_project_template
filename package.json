{
  "name": "vic-vue-template",
  "version": "1.0.0",
  "private": true,
  "author": "ml-Vic <vic@medialand.tw>",
  "license": "MIT",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env NODE_ENV=development APP_ENV=development webpack-dev-server --config internal/webpack/webpack.client.js --hot",
    "build": "cross-env NODE_ENV=production APP_ENV=production webpack --config internal/webpack/webpack.client.js --progress --hide-modules",
    "build-server": "cross-env NODE_ENV=production BABEL_ENV=ssr webpack --config internal/webpack/webpack.server.js --progress --hide-modules",
    "build-ssr": "set SSR=true && npm run build && npm run build-server && node internal/script/SSR",
    "generator": "plop --plopfile ./internal/generator/index.js",
    "gulp": "gulp",
    "storybook": "start-storybook -p 9001 -c ./internal/storybook",
    "test": "cross-env NODE_ENV=test ./node_modules/.bin/jest",
    "flow": "flow",
    "lint-js": "eslint --ext .js --ext .vue .",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.js": "lint-js",
    "*.vue": "lint-js"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "browserslist": [
    "last 2 versions",
    "ie >= 10"
  ],
